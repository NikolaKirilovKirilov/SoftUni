using System;

namespace Conditional_statments_SoftUni
{
    class Program
    {
        static void Main(string[] args)
        {
            string password = "s3cr3t!P@ssw0rd";
            string insPass = Console.ReadLine();
            if (insPass != password)
            {
                Console.WriteLine("Wrong password!");
            }
            else
            {
                Console.WriteLine("Welcome");
            }
        }   
    }
}
